    "connect-sqlite3": "^0.9.13",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "csurf": "^1.11.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-handlebars": "^6.0.6",
    "express-session": "^1.17.3",
    "http-errors": "^2.0.0",
    "mkdirp": "^2.1.5",
    "mongoose": "^6.3.4",
    "morgan": "^1.10.0",
